<Window x:Class="Master_floor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Master_floor"
        mc:Ignorable="d"
        Title="Мастер пол" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="auto" ></ColumnDefinition>
            <ColumnDefinition Width="auto" ></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <StackPanel Grid.Column="0">
            <Button Content="Партнеры" x:Name="PartnersButton" Click="PartnersButton_Click"></Button>
            <Button Content="Материалы"></Button>
        </StackPanel>
        <Frame x:Name="MainFrame"></Frame>
    </Grid>
</Window>
